local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/Neko.lua"))()
local ThemeManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/ThemeManager.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/WhoGodsyoure/Home/refs/heads/main/SaveManager.lua"))()

local Window = Library:CreateWindow({
    Title = "ArchiveXNova",
    Footer = "MOUNT GEMI | YouTube: ArchiveXNova",
    Icon = 85025739024001,
    NotifySide = "Right",
    ShowCustomCursor = true,
    Center = true,
    AutoShow = true,
})

local Tabs = {
    Main = Window:AddTab("Main"),
    ["UI Settings"] = Window:AddTab("Settings")
}

local Group = Tabs.Main:AddLeftGroupbox("Instant Summit")

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local Player = Players.LocalPlayer

local Targets = {
    Vector3.new(-5897.765, 2708.867, -763.761),
    Vector3.new(-6684.920, 3067.994, -802.448)
}

local Event = ReplicatedStorage.Remotes.ResetEvent

local DELAY_BETWEEN = 0.6
local DELAY_AFTER = 0.8
local LOOP_COOLDOWN = 0

local loopConn
local jumpConn
local busy = false

local function getChar()
    return Player.Character or Player.CharacterAdded:Wait()
end

local function getHRP()
    return getChar():WaitForChild("HumanoidRootPart")
end

local function getHumanoid()
    return getChar():FindFirstChildOfClass("Humanoid")
end

local function startSpamJump()
    if jumpConn then return end
    jumpConn = RunService.Heartbeat:Connect(function()
        local hum = getHumanoid()
        if hum then
            hum:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end)
end

local function stopSpamJump()
    if jumpConn then
        jumpConn:Disconnect()
        jumpConn = nil
    end
end

local function teleportSequence()
    if busy then return end
    busy = true

    local hrp = getHRP()

    hrp.CFrame = CFrame.new(Targets[1])
    task.wait(DELAY_BETWEEN)

    hrp.CFrame = CFrame.new(Targets[2])
    task.wait(DELAY_AFTER)

    Event:FireServer()
    busy = false
end

local function stopAll()
    if loopConn then
        loopConn:Disconnect()
        loopConn = nil
    end
    stopSpamJump()
end

Group:AddDropdown("ModeDropdown", {
    Values = {
        "Once",
        "Loop",
        "Stop Loop"
    },
    Default = 1,
    Multi = false,
    Text = "Mode",
    Callback = function(Value)
        if Value == "Once" then
            stopAll()
            startSpamJump()
            teleportSequence()
            task.delay(1, stopSpamJump)

        elseif Value == "Loop" then
            stopAll()
            startSpamJump()
            loopConn = RunService.Heartbeat:Connect(function()
                teleportSequence()
                task.wait(LOOP_COOLDOWN)
            end)

        elseif Value == "Stop Loop" then
            stopAll()
        end
    end
})

