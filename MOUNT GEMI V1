SpawnRoutes.DROP = table.clone(SpawnRoutes.RESET)

local SpeedMap = {
    ["Ultra Slow"] = 9,
    ["Glacial"] = 8,
    ["Snail Pace"] = 7,
    ["Very Slow"] = 6,
    ["Slow"] = 5,
    ["Normal"] = 4,
    ["Moderate"] = 3,
    ["Steady"] = 2.5,
    ["Brisk"] = 2,
    ["Fast"] = 1.6,
    ["Quick"] = 1.3,
    ["Very Fast"] = 1,
    ["Super Fast"] = 0.8,
    ["Ultra Fast"] = 0.6,
    ["Hyper Speed"] = 0.4,
    ["Lightning Fast"] = 0.2,
    ["Warp Speed"] = 0.1,
    ["Instant"] = 0
}

local SpeedValues = {
    "Ultra Slow","Glacial","Snail Pace","Very Slow","Slow",
    "Normal",
    "Moderate","Steady","Brisk","Fast","Quick","Very Fast",
    "Super Fast","Ultra Fast","Hyper Speed","Lightning Fast",
    "Warp Speed","Instant"
}

local function StopAuto()
    AutoLoop = false
    CurrentMode = nil
end

local function StartAuto(mode)
    if AutoLoop then return end
    AutoLoop = true
    CurrentMode = mode

    task.spawn(function()
        while AutoLoop do
            local char = player.Character or player.CharacterAdded:Wait()
            local hrp = char:WaitForChild("HumanoidRootPart")

            for _, pos in ipairs(SpawnRoutes[mode]) do
                if not AutoLoop then return end
                hrp.CFrame = CFrame.new(pos)
                task.wait(CurrentDelay > 0 and CurrentDelay or 0)
            end

            if not AutoLoop then return end

            if mode == "RESET" then
                local resetEvent = ReplicatedStorage:FindFirstChild("ResetEvent")
                if resetEvent then resetEvent:FireServer() end
                local hum = char:FindFirstChildOfClass("Humanoid")
                if hum then hum.Health = 0 end
            elseif mode == "DROP" then
                local dropEvent = ReplicatedStorage:FindFirstChild("CP_DropToStart")
                if dropEvent then dropEvent:FireServer() end
            end

            player.CharacterAdded:Wait()
            task.wait(1.5)
        end
    end)
end

Group:AddDropdown("AutoSpeed", {
    Values = SpeedValues,
    Default = 6,
    Multi = false,
    Text = "Teleport Speed",
    Callback = function(Value)
        SelectedSpeed = Value
        CurrentDelay = SpeedMap[Value] or 4
    end
})

Group:AddDropdown("AutoMode", {
    Values = {"RESET","DROP"},
    Default = 1,
    Multi = false,
    Text = "Mode",
    Callback = function(Value)
        SelectedMode = Value
    end
})

Group:AddDropdown("AutoControl", {
    Values = {"Start","Stop"},
    Default = 1,
    Multi = false,
    Text = "Control",
    Callback = function(Value)
        if Value == "Start" then
            StartAuto(SelectedMode)
        else
            StopAuto()
        end
    end
})

